{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "ConnectionString": "127.0.0.1:6379,password=123456"
  },
  "AppSettings": {
    "MemoryCachingAOP": {
      "Enabled": true
    },
    "RedisCachingAOP": {
      "Enabled": false
    },
    "LogAOP": {
      "Enabled": false
    },
    "SeedDB": true,
    "SeedDBData": true
  },
  "Startup": {
    "ApiName": "Lenceas.Core"
  },
  "Audience": {
    "Secret": "", //不要太短，16位+
    "SecretFile": "C:\\my-file\\ljs.core.audience.secret.txt", //安全。内容就是Secret
    "Issuer": "Lenceas.Core",
    "Audience": "ljs"
  }
}
